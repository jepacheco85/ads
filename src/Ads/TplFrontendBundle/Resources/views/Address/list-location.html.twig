
                <form action="{{ path('user_profile_edit_address') }}" method="POST" novalidate role="form">
                {{ form_row(form.address, {'attr':{'class':'form-control' }}) }}
                {{ form_row(form.country, {'attr':{'class':'selecter-selected' }}) }}
                {{ form_row(form.province, {'attr':{'class':'selecter-selected' }}) }}
                {{ form_row(form.city, {'attr':{'class':'selecter-selected' }}) }}
                {{ form_rest(form) }}
                <button type="submit" class="btn btn-success">
                    Guardar
                </button>
                </form>
            